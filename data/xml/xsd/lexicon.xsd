										</xs:element>
									</xs:sequence>
									<xs:attribute name="id" type="formId" use="required"/>
									<xs:attribute name="norm" type="xs:string" use="required"/>
									<xs:attribute name="morph" type="morph" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="id" type="lemmaId" use="required"/>
						<xs:attribute name="entry" type="xs:string" use="required"/>
						<xs:attribute name="entryID" type="formId" use="required"/>
						<xs:attribute name="pos" type="partOfSpeech" use="required"/>
						<xs:attribute name="gloss" type="xs:string" use="optional"/>
						<xs:attribute name="onp-id" type="onpId" use="optional"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
